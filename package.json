{
  "name": "conductive",
  "version": "0.0.1",
  "description": "Utility components for Express.js",
  "main": "bin/cjs/index.js",
  "module": "bin/mjs/index.js",
  "exports": {
    ".": {
      "import": "./bin/mjs/index.js",
